{
  "manifest_version": 2,
  "name": "Js Helper",
  "short_name": "JsH",
  "description": "Extension for build GTM custom JavaScript variable.",
  "version": "1.1.19",
  "icons": {
    "16": "img/icon16.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "64": "img/icon64.png",
      "128": "img/icon128.png"
    }
  },
  "permissions": [
    "*://*.mozilla.org/*",
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs"
  ],

  "background": {
    "scripts": ["scripts/background.js" ]
  }
}
